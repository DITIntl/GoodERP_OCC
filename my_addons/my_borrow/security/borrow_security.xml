<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="borrow_manager" model="res.groups">
      <field name="name">工装借还管理</field>
      <field name="category_id" ref="base.module_category_hidden"/>
    </record>
    <record model="ir.rule" id="product_template_borrow_rule">
        <field name="name">工装借还适用产品</field>
        <field name="model_id" ref="product.model_product_template"/>
        <field name="domain_force">[('default_code','like','%G%')]</field>
        <field name="groups" eval="[(4,ref('borrow_manager'))]" />
    </record>
  </data>
</odoo>
